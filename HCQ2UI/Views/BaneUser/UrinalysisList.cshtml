@{
    ViewBag.Title = "定期检测结果--档案袋";
}
<div class="wrapper animated fadeInRight autoHeight">
    <div class="row autoHeight">
        <div class="col-sm-12 col-sm-leftRight">
            <div class="example">
                <div class="btn-group hidden-xs" id="exampleToolbar" role="group">
                    @{
                        List<string> folderList = (List<string>)ViewBag.elementList;
                        if (folderList.Contains("urinalysisEdit"))
                        {
                            @*<button id="urinalysisAdd" type="button" class="btn btn-outline btn-primary">
                                    <i class="glyphicon glyphicon-plus" aria-hidden="true">添加</i>
                                </button>*@
                            <button id="urinalysisEdit" type="button" class="btn btn-outline btn-primary">
                                <i class="glyphicon glyphicon-pencil" aria-hidden="true">编辑</i>
                            </button>
                        }
                        if (folderList.Contains("urinalysisDel"))
                        {
                            <button id="urinalysisDel" type="button" class="btn btn-outline btn-danger">
                                <i class="glyphicon glyphicon-trash" aria-hidden="true">删除</i>
                            </button>
                        }
                    }
                </div>
                <table class="table table-striped table-condensed table-hover" data-filter-control="true" id="exampleTableToolbar"></table>
            </div>
        </div>
    </div>
</div>
<!-- 添加/编辑 -->
<div id="orgform" style="width: 580px; overflow: hidden; display: none;">
    <form class="form-horizontal m-t" id="orgFormTable" method="post">
        <div class="form-group">
            <label class="col-sm-4 control-label">本次应到定期检测时间<span class="glyphicon glyphicon-asterisk requiredtitle"></span>：</label>
            <div class="col-sm-8">
                <input id="ur_should_date" name="ur_should_date" type="text" class="laydate-icon form-control input-sm" placeholder="请输入本次应到定期检测时间" title="请输入本次应到尿检时间" onclick="laydate({istime: true, format: 'YYYY-MM-DD'})" required="" aria-required="true" />
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-4 control-label">实际定期检测时间<span class="glyphicon glyphicon-asterisk requiredtitle"></span>：</label>
            <div class="col-sm-8">
                <input id="ur_reality_date" name="ur_reality_date" type="text" class="laydate-icon form-control input-sm" placeholder="请输入实际定期检测时间" title="请输入实际定期检测时间" onclick="laydate({ istime: true, format: 'YYYY-MM-DD hh:mm:ss' })" required="" aria-required="true" />
                <input type="hidden" id="ur_id" name="ur_id" value="" />
                <input type="hidden" id="user_identify" name="user_identify" value="" />
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-4 control-label">现场检测地点：</label>
            <div class="col-sm-8">
                <input id="ur_site" name="ur_site" type="text" class="form-control input-sm" placeholder="请输入现场检测地点" title="请输入现场检测地点" />
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-4 control-label">现场检测方法：</label>
            <div class="col-sm-8">
                <input id="ur_method" name="ur_method" type="text" class="form-control input-sm" placeholder="请输入现场检测方法" title="请输入现场检测方法" />
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-4 control-label">定期检测结果：</label>
            <div class="col-sm-8">
                <input id="ur_result" name="ur_result" type="text" class="form-control input-sm" placeholder="请输入定期检测结果" title="请输入定期检测结果" />
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-4 control-label">结果呈：</label>
            <div class="col-sm-8">
                <input id="ur_result_show" name="ur_result_show" type="text" class="form-control input-sm" placeholder="请输入结果呈" title="请输入结果呈" />
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-4 control-label">检测人：</label>
            <div class="col-sm-8">
                <input id="ur_manager" name="ur_manager" type="text" class="form-control input-sm" placeholder="请输入定期检测监督人员" title="请输入定期检测监督人员" />
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-4 control-label">录单日期：<span class="glyphicon glyphicon-asterisk requiredtitle"></span>：</label>
            <div class="col-sm-8">
                <input id="ur_input_date" name="ur_input_date" type="text" class="laydate-icon form-control input-sm" placeholder="请输入实际定期检测时间" title="请输入实际定期检测时间" onclick="laydate({ istime: true, format: 'YYYY-MM-DD' })" required="" aria-required="true" />
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-4 control-label">任务状态<span class="glyphicon glyphicon-asterisk requiredtitle"></span>：</label>
            <div class="col-sm-8">
                <select id="approve_status" name="approve_status" title="请选择任务状态" required="" aria-required="true" class="form-control show-tick selectpicker" placeholder="请选择任务状态">
                    <option value=0 selected>未完成</option>
                    <option value=1>已完成</option>
                </select>
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-4 control-label">备注：</label>
            <div class="col-sm-8">
                <textarea class="form-control" id="ur_note" name="ur_note" rows="3" placeholder="请输入备注" title="请输入备注"></textarea>
            </div>
        </div>
    </form>
</div>
@section footSection{
    <script src="~/Resources/myScript/BaneUser/bagBaneManager.js"></script>
    <script>
            $(function () {
                //初始化Page
                urinalysisMannager.initPage();
            });
    </script>
}

